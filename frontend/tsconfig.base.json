{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": ["ESNext", "DOM"],
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "react": ["vendor/react"],
      "react/*": ["vendor/react/*"],
      "react-dom": ["vendor/react-dom"],
      "react-dom/*": ["vendor/react-dom/*"],
      "antd": ["vendor/antd"],
      "antd/*": ["vendor/antd/*"]
    },
    "types": []
  }
}
